{
  "name": "puppeteer-csv-import",
  "version": "1.0.0",
  "description": "Automated CSV import system using Puppeteer",
  "main": "src/main.js",
  "scripts": {
    "start": "HEADLESS=false node src/main.js",
    "dev": "node --inspect src/main.js",
    "test": "node src/main.js --dry-run",
    "debug": "DEBUG_HTML=true HEADLESS=false node src/main.js",
    "fast": "HEADLESS=false node scripts/fast-import-helper.js",
    "check-history": "HEADLESS=false node scripts/history-validator.js",
    "validate-data": "HEADLESS=false node scripts/automated-data-validator.js",
    "record-pagination": "HEADLESS=false node scripts/record-pagination.js",
    "record-validation": "HEADLESS=false node scripts/record-validation-flow.js",
    "verify": "node scripts/verify-setup.js"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "path": "^0.12.7",
    "puppeteer": "^24.9.0",
    "winston": "^3.8.0"
  },
  "keywords": [
    "puppeteer",
    "automation",
    "csv",
    "import"
  ],
  "author": "Your Name",
  "license": "MIT"
}